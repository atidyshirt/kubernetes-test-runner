{
  "name": "@atidyshirt/node-devspace-testing",
  "version": "0.0.0",
  "description": "Testing utilities for DevSpace Kubernetes development",
  "type": "module",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "devspace",
    "kubernetes",
    "testing",
    "kubectl"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^20.0.0",
    "mongodb": "7.0.0",
    "semantic-release": "^25.0.2",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=24"
  }
}
